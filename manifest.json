{
  "manifest_version": 3,
  "name": "imood better forum",
  "description": "an extension to make the forum on imood look nicer",
  "version": "1.0.1",

  "content_scripts": [
    {
      "matches": ["*://www.imood.com/forums/*/threads/*"],
      "js": ["forum.js"]
    }
  ],

  "browser_specific_settings": {
  "gecko": {
    "id": "extensionemail????idk@website.com",
    "strict_min_version": "42.0"
  }
}

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}
